<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- com.green.board.mapper -->
<mapper namespace='com.green.carproduct.mapper.CarProductMapper'>
	<select id="getAllCarProduct" resultType="carDTO">
		SELECT * FROM carProduct
	</select>
	
	<!-- public void insertCarProduct(CarProductDTO dto); -->
	<insert id="insertCarProduct" parameterType="carDTO">
		INSERT INTO carProduct(carName,price,company,img,info)
		VALUES(#{carName},#{price},#{company},#{img},#{info})
	</insert>
	
</mapper>